<h2>List</h2>

{{#each model as |library|}}
<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">{{library.name}}
		</h3>
	</div>
	<div class="panel-body">
		<p class="address">Address: <span class="addressSpan">{{library.address}}</span></p>
		<p class="phone">Phone: <span class="phoneSpan">{{library.phone}}</span></p>
	</div>
</div>

{{/each}}